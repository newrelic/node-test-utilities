{
  "name": "@newrelic/test-utilities",
  "version": "0.0.0",
  "description": "Test library for New Relic instrumentation modules.",
  "main": "index.js",
  "scripts": {
    "docs": "rm -r docs; jsdoc -c ./jsdoc-conf.json -d docs -r *.js lib/",
    "lint": "eslint *.js lib/*",
    "test": "npm run unit",
    "unit": "tap tests/unit/*.tap.js tests/unit/*/*.tap.js"
