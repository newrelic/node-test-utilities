    }
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "eslint": "^3.13.1",
    "jsdoc": "^3.4.0",
    "newrelic": "^1.28.1",
    "tap": "^9.0.3"
  },
  "dependencies": {
    "async": "^2.1.4",
    "colors": "^1.1.2",
    "concat-stream": "^1.6.0",
